{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    // "extends": ["eslint:recommended", "plugin:react/recommended"],
    "extends": ["airbnb"],
    "plugins": ["import", "react-hooks"],
    "rules": {
        "no-underscore-dangle": "off"
    }
}
